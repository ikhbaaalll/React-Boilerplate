{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"parser": "flow",
	"plugins": ["prettier-plugin-tailwindcss"],
	"printWidth": 80,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true
}
